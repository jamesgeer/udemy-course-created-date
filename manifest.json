{
	"manifest_version": 2,
	"name": "Udemy Course Date",
	"description": "Display the creation date of the course your are currently viewing.",
	"version": "1.0",
	"homepage_url": "https://github.com/jamesgeer/udemy-course-date",
	"permissions": [
		"tabs", "activeTab", "webRequest", "*://*.udemy.com/*"
	],
	"content_scripts": [
		{
			"matches": ["*://*.udemy.com/*"],
			"js": ["./src/index.ts"]
		}
	]
}